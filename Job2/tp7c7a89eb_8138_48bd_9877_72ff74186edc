#!/bin/sh
MDCE_DECODE_FUNCTION=parallel.cluster.generic.communicatingDecodeFcn
export MDCE_DECODE_FUNCTION
MDCE_STORAGE_CONSTRUCTOR=makeFileStorageObject
export MDCE_STORAGE_CONSTRUCTOR
MDCE_JOB_LOCATION=Job2
export MDCE_JOB_LOCATION
MDCE_MATLAB_EXE=/shared/MATLAB/R2018a/bin/worker
export MDCE_MATLAB_EXE
MDCE_MATLAB_ARGS=-parallel
export MDCE_MATLAB_ARGS
MDCE_DEBUG=true
export MDCE_DEBUG
MLM_WEB_LICENSE=false
export MLM_WEB_LICENSE
MDCE_STORAGE_LOCATION=/home/vkanoria/Documents/vkanoria/R2018a
export MDCE_STORAGE_LOCATION
MDCE_CMR=/shared/MATLAB/R2018a
export MDCE_CMR
MDCE_TOTAL_TASKS=8
export MDCE_TOTAL_TASKS
qsub -N Job2 -j oe -o '/home/vkanoria/Documents/vkanoria/R2018a/Job2/Job2.log' -v MDCE_DECODE_FUNCTION,MDCE_STORAGE_CONSTRUCTOR,MDCE_JOB_LOCATION,MDCE_MATLAB_EXE,MDCE_MATLAB_ARGS,MDCE_DEBUG,MLM_WEB_LICENSE,MDCE_STORAGE_LOCATION,MDCE_CMR,MDCE_TOTAL_TASKS -l nodes=4:ppn=2 '/home/vkanoria/Documents/vkanoria/R2018a/Job2/communicatingJobWrapper.sh'
